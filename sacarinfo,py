import sqlite3

conn = sqlite3.connect("database.db")
cursor = conn.cursor()


cursor.execute("SELECT id_usuario, fecha, puntuacion FROM Resultados WHERE DATE(fecha) = DATE('now');")
a = cursor.fetchall()


print("Usuarios en la base de datos:")
for u in a:
    print(u)
    
# for u in respuestas:
#  print(u)

conn.close()